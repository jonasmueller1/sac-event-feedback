<div class="widget">
    <h2>Event Auswertung für {{ event.title }}</h2>

    <div class="sefb-feedback-count">Anzahl Rückmeldungen: {{ feedback_count }}</div>

    {% for feedback in feedbacks.optionFields %}
        <h2 class="sefb-eval-question">{{ feedback.label }}</h2>
        <ul>
        {% for option in feedback.values %}
            <li>{{ option.count }}x {{ option.label }}</li>
        {% endfor %}
        </ul>
    {% endfor %}

    {% for feedback in feedbacks.textareaFields %}
        <h2 class="sefb-eval-question">{{ feedback.label }}</h2>
        <ul>
            {% for value in feedback.values %}
                <p>{{ value }}</p>
            {% endfor %}
        </ul>
    {% endfor %}

</div>